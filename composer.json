    "endroid/qr-code": "Bibliothèque de création de QR Code",
    "guzzlehttp/guzzle": "Client PHP Http",
    "league/commonmark": "Markdown parser conforme à la specification CommonMark",
    "mpratt/embera": "Bibliothèque client d'encapsulation de vidéo",
    "mrclay/minify": "Minification avancée des scripts CSS et JS",
    "mustache/mustache": "Gestionnaire de templates PHP",
    "neitanod/forceutf8": "Forçage d'encodage en utf-8 d'une chaine de caractères",
    "phpdocumentor/reflection-docblock": "Analyse des annotation PHP DocBlock",
    "vimeo/vimeo-api": "Client PHP Api Vimeo"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "tiFy\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "bower install --allow-root --save"
    ],
    "post-update-cmd": [
      "bower install --allow-root --save"
    ]
  },
  "bin": [
    "bin/bee"
  ]
